h1= user.name

#newAddress.panel.panel-default.col-xs-6.col-sm-4
  .panel-body
    .form-group
      label(for="location") Location
      .input-group
        input#location.form-control(type="text", placeholder="City, State")
        span.input-group-addon
          span.glyphicon.glyphicon-map-marker
    .form-group
      label(for="description") Description
      textarea#description.form-control(placeholder="Description")
    .form-group
      button#btnAdd.btn.btn-primary Add
#listings(ng-app="thoughtbnb", ng-controller="ListingsController")
  .listing.panel.panel-default.col-xs-6.col-sm-4(ng-repeat="listing in listings")
    .panel-body
      h3 {{listing.location}}
      p {{listing.description}}
script(src="/scripts/userHome.js")
script(src="/scripts/listingsController.js")
